{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "plugins": [
        "prettier",
        "react-hooks"
    ],
    "rules": {
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "warn",
        "prettier/prettier": [
            "error"
        ]
    }
}
